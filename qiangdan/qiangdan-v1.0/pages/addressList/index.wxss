page {
    padding-bottom: 180rpx;
    height: 100%;
}

.container {
    margin-top: 10rpx;
}

.content {
    width: 690rpx;
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #ccc;
    background-color: #fff;
}

.content-top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #000;
    font-size: 28rpx;
    line-height: 46rpx;
}

.content-tel {
    flex: 1;
    margin-left: 20rpx;
}

.content-bottom {
    color: #FFBFBFBF;
    font-size: 24rpx;
    line-height: 46rpx;
}

.blanks {
    width: 100%;
    height: 70%;
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    display: flex;
    justify-content: center;
    align-items: center;
}

.addAddress {
    position: fixed;
    left: 25rpx;
    right: 25rpx;
    bottom: 20rpx;
    height: 80rpx;
    line-height: 82rpx;
    border-radius: 8rpx;
    background-color: #81D8D0;
    color: rgba(255, 255, 255, 1);
    font-size: 28rpx;
    text-align: center;
}