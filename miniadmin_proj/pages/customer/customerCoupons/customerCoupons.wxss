.couponsbox {
    width: 100%;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    flex-direction: column;
    padding-bottom: 100rpx;
}

@import '../../template/coupon.wxss';


/*btnbox*/

.btnbox {
    background-color: #4f9dd0;
    width: 100%;
    height: 90rpx;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    line-height: 90rpx;
    font-size: 30rpx;
    color: #ffffff;
    display: flex;
    justify-content: center;
    align-items: center;
}

.btn {
    display: flex;
    justify-content: center;
    align-items: baseline;
}

.jia {
    font-size: 42rpx;
}